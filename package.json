    "express": "^4.17.1",
    "joi": "^17.5.0",
    "node-fetch": "^3.1.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "prettier": "^2.5.1",
    "sequelize": "^6.12.0",
    "sequelize-cli": "^6.3.0"
